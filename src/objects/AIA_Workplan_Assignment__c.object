    <validationRules>
        <fullName>EWPA001</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED( Due_Date__c ),OR(( $Profile.Name  &lt;&gt; &apos;System Administrator&apos;  ),($User.Id &lt;&gt; CreatedBy.Id)))</errorConditionFormula>
        <errorMessage>EWPA001: Only Administrators or the creator of an Assignment can change the Due Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EWPA002</fullName>
        <active>true</active>
        <errorConditionFormula>AND(External_Assignee__c != &apos;&apos; , Internal_Assignee__c != null)</errorConditionFormula>
        <errorMessage>EWPA002: Please select only one external or Internal Assignee.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EWPA003</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE( Status__c ),&quot;Completed&quot;)&amp;&amp; ISCHANGED( Status__c )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>EWPA003: User cannot change the status once it is &quot;Completed&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EWPA004</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE( Status__c ),&quot;Not Required&quot;)&amp;&amp; ISCHANGED( Status__c )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>EWPA004: User cannot change the status once it is &quot;Not Required&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EWPA005</fullName>
        <active>true</active>
        <description>The assignment can not be set to not required if the assignment is required</description>
        <errorConditionFormula>Workplan_Type_Task__r.Required__c &amp;&amp;  TEXT(Status__c) = &apos;Not Required&apos;</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>EWPA005: If assignment is required the status can not be set to &quot;Not Required&quot;</errorMessage>
    </validationRules>

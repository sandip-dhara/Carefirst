    <validationRules>
        <fullName>EEvent001</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
AND( 
ISPICKVAL (Meeting_Type__c, &apos;Benefit Design Review Meeting&apos;), 
ISBLANK(  Benefit_Design__c  )), True, False 
)</errorConditionFormula>
        <errorDisplayField>Benefit_Design__c</errorDisplayField>
        <errorMessage>EEvent001: When Meeting Type is &apos;Benefit Design Review Meeting&apos;. Benefit Design is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Bid_Comments_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND (ISPICKVAL( Bid_Meeting_Decision__c , &apos;No Bid&apos;) , ISBLANK( No_Bid_Comments__c ) )</errorConditionFormula>
        <errorDisplayField>No_Bid_Comments__c</errorDisplayField>
        <errorMessage>Please enter No Bid Comments</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Send_Email</fullName>
        <availability>online</availability>
        <displayType>button</displayType>

    </webLinks>
    <webLinks>
        <fullName>Sign_Up</fullName>
        <availability>online</availability>
        <description>Button to open enrollment meeting details page</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Sign Up</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)} 
try{
if({! ISPICKVAL(Event.Meeting_Status1__c, &apos;Full&apos;) } ){
alert(&apos;This enrollment meeting has already reached capacity.&apos;);  
} 
else if({!ISPICKVAL(Event.Meeting_Status1__c, &apos;Completed&apos;)}){ 
alert(&apos;This enrollment meeting has already been completed&apos;); 
} 
else{ 
var result = sforce.apex.execute(&quot;AIA_OpenEnrollmentMeetingSignUp&quot;,&quot;signUp&quot;,{eventId:&quot;{!Event.Id}&quot;}); 
} 
}
catch(err){
alert(&apos;You have already signed up for the meeting or the meeting date is over&apos;);
}
//alert(result); 
window.location.reload();</url>
    </webLinks>

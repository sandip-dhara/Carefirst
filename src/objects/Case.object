    <validationRules>
        <fullName>ECSE021</fullName>
        <active>true</active>
        <description>if Implementation is entered, then Issue Category are required</description>
        <errorConditionFormula>RecordType.Name  =&apos;AIA Account Issue&apos;&amp;&amp; IF(   Implementation__c = null &amp;&amp; Implementation__c = &apos;&apos; ,false,IF(ISBLANK(TEXT( Issue_Category__c )),true,false))</errorConditionFormula>
        <errorDisplayField>Issue_Category__c</errorDisplayField>
        <errorMessage>ECSE021 : If Implementation field is not blank than Issue Category field cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ECSE022</fullName>
        <active>true</active>
        <description>If Issue Frequency is Global then Account cannot have a value</description>
        <errorConditionFormula>RecordType.Name =&apos;AIA Account Issue&apos; &amp;&amp; IF( ISPICKVAL( Issue_Frequency__c ,&apos;Global&apos;),IF(  AccountId &lt;&gt;  null &amp;&amp;  AccountId &lt;&gt; &apos;&apos; ,true,false),false)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>ECSE022 : If Issue Frequency is &quot;Global&quot; then Account cannot have a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ECSE023</fullName>
        <active>true</active>
        <description>if Implementation is entered, then Issue Category are required</description>
        <errorConditionFormula>RecordType.Name  =&apos;AIA Account Issue&apos; &amp;&amp; IF(  Implementation__c = null &amp;&amp; Implementation__c = &apos;&apos; ,false,IF(ISBLANK(TEXT(  Issue_Sub_Category__c  )),true,false))</errorConditionFormula>
        <errorDisplayField>Issue_Sub_Category__c</errorDisplayField>
        <errorMessage>ECSE023 : If Implementation field is not blank than Issue Sub Category field cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ECSE024</fullName>
        <active>true</active>
        <description>If Issue Frequency is Account Specific then Account cannot be blank</description>
        <errorConditionFormula>RecordType.Name =&apos;AIA Account Issue&apos; &amp;&amp; IF( ISPICKVAL( Issue_Frequency__c ,&apos;Account Specific&apos;),IF(  AccountId =  null &amp;&amp;  AccountId = &apos;&apos; ,true,false),false)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>ECS024 : If Issue Frequency =&apos;&quot;Account Specific&quot; then Account cannot be blank</errorMessage>
    </validationRules>
